        "react-hot-loader/babel",
        "ramda",
        "transform-class-properties"
    ],
    "presets": [
        "@babel/preset-env",
        ["es2015", { "modules": "commonjs" }],
        "react",
        "stage-0"
    ],
    "env": {
        "test": {
            "plugins": [
                "react-hot-loader/babel",
                "transform-class-properties",
                "dynamic-import-node"
            ],
